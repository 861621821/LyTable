{
  "生成lyTable组件tableOpt属性": {
    "prefix": "tableOpt",
    "body": [
      "tableOpt () {",
        "\treturn {",
          "\t\tcolumn: [",
            "\t\t\t{",
              "\t\t\t\tlabel: '${1}',",
              "\t\t\t\tprop: '',",
            "\t\t\t},",
          "\t\t]",
        "\t}",
      "}"
    ],
    "description": "生成lyTable组件tableOpt属性"
  },
  "引入lyTabel公共代码": {
    "prefix": "import",
    "body": [
      "import lyTable from '@/components/mixins/ly-table.js'"
    ],
    "description": "引入lyTabel公共代码"
  },
  "配置表格列": {
    "prefix": "column",
    "body": [
      "column: [",
      "\t{",
        "\t\t",
      "\t}",
      "]"
    ],
    "description": "配置表格列"
  },
  "表头label/搜索label：String": {
    "prefix": "label",
    "body": [
      "label: '${1}'"
    ],
    "description": "表头label/搜索label：String"
  },
  "列对应字段/搜索字段：String": {
    "prefix": "prop",
